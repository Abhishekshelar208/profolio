        }
      },
      "macos": {
        "default": {
          "projectId": "profolio-abhishek-shelar",
          "appId": "1:1076236209612:ios:b779d9532e917dce7aa169",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "profolio-abhishek-shelar",
          "configurations": {
            "android": "1:1076236209612:android:52b191d53e03491a7aa169",
            "ios": "1:1076236209612:ios:b779d9532e917dce7aa169",
            "macos": "1:1076236209612:ios:b779d9532e917dce7aa169",
            "web": "1:1076236209612:web:b6f8e0138810f0107aa169",
            "windows": "1:1076236209612:web:9a037aa5b43216887aa169"
